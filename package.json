{
  "name": "lion-byte.com",
  "version": "2.0.0",
  "description": "Mark Hernandez's portfolio site",
  "author": "Mark Hernandez <mark@lion-byte.com> (https://www.lion-byte.com)",
  "scripts": {
    "build": "eleventy --quiet",
    "dev": "eleventy --serve",
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|less|css|ts|md)\"",
    "test": "npm run type && jest",
    "type": "tsc --pretty"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MarkH817/lion-byte.com.git"
  },
  "keywords": [
    "blog",
    "personal site"
  ],
  "main": "n/a",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@lionbyte/eslint-config": "^1.8.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "babel-jest": "^26.3.0",
    "date-fns": "^2.16.0",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-runner-eslint": "^0.10.0",
    "less": "^3.12.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "prismjs": "^1.21.0",
    "turretcss": "^5.2.2",
    "typeface-nunito": "1.1.3",
    "typeface-open-sans": "0.0.75"
  }
}
