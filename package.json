{
  "name": "lion-byte.com",
  "version": "2.0.0",
  "description": "Mark Hernandez's portfolio site",
  "author": "Mark Hernandez <mark@lion-byte.com> (https://www.lion-byte.com)",
  "scripts": {
    "build": "run-s build:* --print-label",
    "build:11ty": "eleventy --quiet",
    "build:webpack": "webpack --config webpack/webpack.prod.js",
    "dev": "run-p dev:* --print-label",
    "dev:11ty": "eleventy --serve",
    "dev:webpack": "webpack --config webpack/webpack.dev.js --watch",
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|less|css|ts|md)\"",
    "test": "npm run type && jest",
    "type": "tsc --pretty"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MarkH817/lion-byte.com.git"
  },
  "keywords": [
    "blog",
    "personal site"
  ],
  "main": "n/a",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@lionbyte/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.14",
    "date-fns": "^2.29.3",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "jest-runner-eslint": "^1.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "core-js": "^3.27.2",
    "modern-normalize": "^1.1.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "regenerator-runtime": "^0.13.11",
    "typeface-nunito": "1.1.13",
    "typeface-open-sans": "1.1.13"
  }
}
