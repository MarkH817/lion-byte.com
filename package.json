{
  "name": "lion-byte.com",
  "version": "2.0.0",
  "description": "Mark Hernandez's portfolio site",
  "author": "Mark Hernandez <mark@lion-byte.com> (https://www.lion-byte.com)",
  "scripts": {
    "build": "run-s build:* --print-label",
    "build:11ty": "eleventy --quiet",
    "build:webpack": "webpack --config webpack/webpack.prod.js",
    "dev": "run-p dev:* --print-label",
    "dev:11ty": "eleventy --serve",
    "dev:webpack": "webpack --config webpack/webpack.dev.js --watch",
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|less|css|ts|md)\"",
    "test": "npm run type && jest",
    "type": "tsc --pretty"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MarkH817/lion-byte.com.git"
  },
  "keywords": [
    "blog",
    "personal site"
  ],
  "main": "n/a",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@lionbyte/eslint-config": "^1.8.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/webpack": "^4.41.21",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-merge": "^4.1.5",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "date-fns": "^2.16.0",
    "eslint": "^7.7.0",
    "file-loader": "^6.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-runner-eslint": "^0.10.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "lint-staged": "^10.2.13",
    "mini-css-extract-plugin": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.1",
    "styles-loader": "^2.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.3"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "modern-normalize": "^1.0.0",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.21.0",
    "regenerator-runtime": "^0.13.7",
    "typeface-nunito": "1.1.3",
    "typeface-open-sans": "0.0.75"
  }
}
