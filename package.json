{
  "name": "lion-byte.com",
  "version": "2.0.0",
  "description": "Mark Hernandez's portfolio site",
  "author": "Mark Hernandez <mark@lion-byte.com> (https://www.lion-byte.com)",
  "scripts": {
    "build": "run-s build:* --print-label",
    "build:11ty": "eleventy --quiet",
    "build:webpack": "webpack --config webpack/webpack.prod.js",
    "dev": "run-p dev:* --print-label",
    "dev:11ty": "eleventy --serve",
    "dev:webpack": "webpack --config webpack/webpack.dev.js --watch",
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|less|css|ts|md)\"",
    "test": "npm run type && jest",
    "type": "tsc --pretty"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MarkH817/lion-byte.com.git"
  },
  "keywords": [
    "blog",
    "personal site"
  ],
  "main": "n/a",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@lionbyte/eslint-config": "^2.0.0-next.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/cssnano": "^4.0.1",
    "@types/jest": "^27.0.3",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.12",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.4",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.12",
    "date-fns": "^2.27.0",
    "eslint": "^8.4.1",
    "husky": "^4.3.0",
    "jest": "^27.4.4",
    "jest-runner-eslint": "^1.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "core-js": "^3.19.3",
    "haunted": "^4.8.2",
    "lit": "^2.0.2",
    "modern-normalize": "^1.1.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.25.0",
    "regenerator-runtime": "^0.13.9",
    "typeface-nunito": "1.1.13",
    "typeface-open-sans": "1.1.13"
  }
}
