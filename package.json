  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.js && standard --fix",
    "format:less": "prettier --single-quote --write src/**/*.less",
    "test": "standard ."
  },
  "husky": {
    "pre-commit": {
      "*.js": [
        "prettier --write",
        "standard --fix",
        "git add"
      ],
      "*.{md,less}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "standard": "^11.0.1"
  }
