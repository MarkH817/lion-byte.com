{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "plugins": [{ "name": "typescript-styled-plugin", "validate": false }],
    "target": "esnext"
  },
  "exclude": [".cache/", "coverage/", "node_modules/", "dist/"]
}
